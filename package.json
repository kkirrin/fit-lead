{
  "name": "fit-lead-root",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "npm-run-all --parallel start:back start:front",
    "start:back": "npm --prefix back run dev",
    "start:front": "npm --prefix front run dev",
    "postinstall": "npm --prefix back ci || npm --prefix back i && npm --prefix front ci || npm --prefix front i"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}


